Algoritmo ejercicio_5
	Escribir ("Digite numero_uno")
	Leer Numero_uno
	Escribir ("Digite numero_dos")
	Leer Numero_dos
	suma<- Numero_uno+Numero_dos
	resta<- Numero_uno-Numero_dos
	division<- Numero_uno/Numero_dos
	multiplicacion<- Numero_uno*Numero_dos
